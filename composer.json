{
  "name": "plaisio/cookie",
  "description": "PhpPlaisio: Cookie",
  "keywords": [
    "PhpPlaisio",
    "Cookie"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "setbased/exception": "^2.6.0"
  },
  "require-dev": {
    "ext-dom": "*",
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "plaisio/console-kernel": "^1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Cookie\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Cookie\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
